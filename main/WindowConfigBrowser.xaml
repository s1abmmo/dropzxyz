<Window x:Class="main.WindowConfigBrowser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:main"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Height="285" Width="276">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    Source="Dictionary1.xaml">
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid MouseDown="Config_MouseDown"  Name="config" Margin="0,0,2,6">
        <Grid.Resources>
            <local:ComparisonConverter x:Key="ComparisonConverter" />
        </Grid.Resources>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="124*"/>
            <ColumnDefinition Width="143*"/>
        </Grid.ColumnDefinitions>
        <TextBox Name="TextBoxUserAgent" HorizontalAlignment="Left" Height="61" Margin="10,36,0,0" TextWrapping="Wrap" Text="{Binding UserAgent}" VerticalAlignment="Top" Width="249" Grid.ColumnSpan="2" />
        <Label Content="Useragent" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Network" HorizontalAlignment="Left" Margin="10,97,0,0" VerticalAlignment="Top"/>
        <TextBox Name="Host"  IsEnabled="{Binding Proxytype, Converter={StaticResource ComparisonConverter}, ConverterParameter={x:Static local:ProxyType.socks5}}" HorizontalAlignment="Left" Height="23" Margin="59,137,0,0" TextWrapping="Wrap" Text="{Binding Host}" VerticalAlignment="Top" Width="101" Grid.ColumnSpan="2" TextChanged="Host_TextChanged"/>
        <TextBox Name="Port" IsEnabled="{Binding Proxytype, Converter={StaticResource ComparisonConverter}, ConverterParameter={x:Static local:ProxyType.socks5}}" HorizontalAlignment="Left" Height="23" Margin="78,136,0,0" TextWrapping="Wrap" Text="{Binding Port}" VerticalAlignment="Top" Width="57" Grid.Column="1" TextChanged="Port_TextChanged"/>
        <Label Content="Host"  HorizontalAlignment="Left" Margin="10,133,0,0" VerticalAlignment="Top"/>
        <Label Content="Port"  HorizontalAlignment="Left" Margin="41,133,0,0" VerticalAlignment="Top" RenderTransformOrigin="2.158,-0.615" Grid.Column="1"/>
        <Button Content="Apply" HorizontalAlignment="Left" Margin="85,206,0,0" VerticalAlignment="Top" Width="75" Height="37" Grid.ColumnSpan="2" Style="{StaticResource ButtonBlue}" Click="Button_Click"/>
        <Button Content="Close" HorizontalAlignment="Left" Margin="60,206,0,0" VerticalAlignment="Top" Width="75" Height="37" Grid.Column="1" RenderTransformOrigin="0.467,0.243" Style="{StaticResource ButtonBlue}" Click="Button_Click_1"/>
        <RadioButton Name="None" Content="none" HorizontalAlignment="Left" Margin="10,165,0,0" VerticalAlignment="Top" IsChecked="{Binding Proxytype, Converter={StaticResource ComparisonConverter}, ConverterParameter={x:Static local:ProxyType.none}}" Checked="RadioButton_Checked"/>
        <RadioButton Name="Socks5" Content="socks5" HorizontalAlignment="Left" Margin="10,185,0,0" VerticalAlignment="Top" IsChecked="{Binding Proxytype, Converter={StaticResource ComparisonConverter}, ConverterParameter={x:Static local:ProxyType.socks5}}" RenderTransformOrigin="0.412,-0.667" Checked="Socks5_Checked"/>

    </Grid>
</Window>
